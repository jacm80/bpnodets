      "eslint": "^6.1.0",
      "mocha": "^6.2.0",
      "should": "^13.2.3",
      "supertest": "^4.0.2",
      "ts-jest": "^24.0.2",
      "ts-node": "^8.3.0",
      "typescript": "^3.3.3333"
   },
   "dependencies": {
      "body-parser": "^1.19.0",
      "class-validator": "^0.9.1",
      "compression": "^1.7.4",
      "cors": "^2.8.5",
      "crypto": "^1.0.1",
      "custom-env": "^1.0.2",
      "express": "^4.17.1",
      "helmet": "^3.20.0",
      "http": "^0.0.0",
